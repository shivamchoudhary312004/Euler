import java.io.*;
class Mult
{
 public static void main(String args[])
 {
    BufferedReader in= new BufferedReader
    (new InputStreamReader(System.in));
     int i,s=0;
     for(i=1;i<1000;i++)
     {
      if((i%3==0)||(i%5==0))
        s=s+i;}
    System.out.println("The sum of all multipes of 3&5 below 1000="+s);
 }
}
