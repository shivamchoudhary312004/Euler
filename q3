import java.io.*;
class multiple
{
     public static void main(String args[])
     {
        BufferedReader in = new BufferedReader
        (new InputStreamReader(System.in));
        int a=1,i,x;
      while(true)
     {
        x=0;
        for(i=1;i<=20;i++)
        {
         if(a%i==0)
         x++;
         
       }if(x==20)
         break;
        a++;
   
      }
      System.out.print(a);
     }
}
        
        
        
      
     

 

