import java.io.*;
class Fibo
{
 public static void main(String args[])
 {
    BufferedReader in= new BufferedReader
    (new InputStreamReader(System.in));
    int i,n=2,s=0; long s2=2;
    for (i=1;i<=4000000;)
    {s=i+n;
    i=n;
    n=s;
    if(s%2==0)
    s2=s2+s;
    }
   System.out.println("Sum of even value of Fibo="+s2);
 }
}
