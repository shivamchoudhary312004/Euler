import java.io.*;
class six-sum
{
    public static void main(String args[])
    {
        BufferedReader in= new BufferedReader
        (new InputStreamReader(System.in));
        int i,s=0,s1=0,diff=0,a=0;
        for(i=1;i<=100;i++)
        {
            s= s+i*i;
            s1=s1+i;
        }
        diff=s1*s1-s;
        System.out.println("Differece="+diff);
    }
}
